<Window x:Class="ManagerSQLiteWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ManagerSQLiteWPF"
        mc:Ignorable="d"
        Title="Granja" Height="350" Width="525" Icon="/ManagerSQLiteWPF;component/Images/cow_head.png" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <ListBox Grid.Column="0" ItemsSource="{Binding LstVacas}">
            
        </ListBox>
        <ListBox Grid.Column="1" ItemsSource="{Binding LstTractores}">

        </ListBox>
        <ListBox Grid.Column="2" ItemsSource="{Binding LstGastos}">

        </ListBox>
    </Grid>
</Window>
